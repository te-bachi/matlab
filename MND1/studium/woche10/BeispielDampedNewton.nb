Cell[BoxData[
 RowBox[{
  RowBox[{"NewtonStep", "[", 
   RowBox[{"x_", ",", "x0_", ",", "\[Sigma]0_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "\[Lambda]", ",", "C\[Lambda]", ",", "s2", ",", "xn", ",", "sk", ",", 
      "fk"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Lambda]", "=", "1."}], ";", "\[IndentingNewLine]", 
     RowBox[{"C\[Lambda]", "=", 
      RowBox[{"1", "-", 
       FractionBox["\[Lambda]", "4"]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"sk", "=", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"df", "[", 
         RowBox[{"x", ",", "x0", ",", "\[Sigma]0"}], "]"}], 
        RowBox[{"-", "1"}]], 
       RowBox[{"f", "[", 
        RowBox[{"x", ",", "x0", ",", "\[Sigma]0"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"xn", "=", 
      RowBox[{"x", "-", 
       RowBox[{"\[Lambda]", " ", "sk"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"s2", "=", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"df", "[", 
         RowBox[{"x", ",", "x0", ",", "\[Sigma]0"}], "]"}], 
        RowBox[{"-", "1"}]], 
       RowBox[{"f", "[", 
        RowBox[{"xn", ",", "x0", ",", "\[Sigma]0"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Abs", "[", "s2", "]"}], "\[GreaterEqual]", 
          RowBox[{"C\[Lambda]", " ", 
           RowBox[{"Abs", "[", "sk", "]"}]}]}], ")"}], "&&", 
        RowBox[{"(", 
         RowBox[{"\[Lambda]", "\[GreaterEqual]", "\[Lambda]min"}], ")"}]}], 
       ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\[Lambda]", "=", 
         RowBox[{"\[Lambda]", "/", "2"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"C\[Lambda]", "=", 
         RowBox[{"1", "-", 
          FractionBox["\[Lambda]", "4"]}]}], ";", "\[IndentingNewLine]", 
